{
  "name": "kintone-plugin",
  "version": "1.0.0",
  "description": "q",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dts-gen": "npx @kintone/dts-gen --app-id $KINTONE_APP_ID --domain $KINTONE_DOMAIN",
    "build:files": "webpack --mode=production --env skipPlugin=true",
    "ensure:ppk": "[ ! -f private.ppk ] && (kintone-plugin-packer dist && mv *.ppk private.ppk && rm -f tab-plugin.zip) || echo 'Using existing private.ppk'",
    "build:zip": "kintone-plugin-packer dist --ppk private.ppk",
    "rename:zip": "mv plugin.zip settings-copy-plugin.zip",
    "build": "npm run build:files && npm run ensure:ppk && npm run build:zip && npm run rename:zip",
    "dev": "webpack serve --mode development"
  },
  "author": "",
  "license": "ISC",
